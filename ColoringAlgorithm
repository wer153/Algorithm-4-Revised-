int vcolor[26];
int n=25, m=4, count=0;
bool promising(int i)
{
	int j = 1;
	bool _switch = true;
	
	while(j<i && _switch)
	{
		if(W[i][j] && vcolor[i]==vcolor[j])
			_switch = false;
		j++;
	}
	return _switch;
}
void m_coloring (int i)
{
	int color;
	if(promising(i))
		if(i==n)
		{
			for(int j=1; j<=n;j++)
				cout<<vcolor[j]<<" ";
			count++;
		}
		else
			for(color = 1; color<=m; color++)
			{
				vcolor[i+1] = color;
				m_coloring(i+1);
			}
}

int main()
{
	void m_coloring(int i);
	m_coloring(0);
	cout<<"\n\n 최소 4개의 컬러로 서울지도로 칠할 수 있습니다.”<<endl;
	cout<<count<<"20158464개의 조합이 생성됩니다."<<endl;

}
